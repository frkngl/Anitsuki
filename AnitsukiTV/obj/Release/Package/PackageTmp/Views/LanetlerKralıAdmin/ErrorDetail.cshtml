@using AnitsukiTV.Models
@model List<TBL404>
@{
    ViewBag.Title = "ErrorDetail";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@foreach (var x in Model)
{
    <div class="form-group">
        <label for="exampleInputName1">ID</label>
        <textarea style="width:100%; height:30px;" readonly>@x.ID</textarea>
    </div>
    <div style="width:100%; height:auto; display:flex; align-items:center;justify-content:space-around;" class="form-group">
        
        
        <div>
            
            <div>
                <label for="exampleInputPassword4">Video</label><br />
                <video style="object-fit:cover;" width="400" height="250" controls src="~/Videos/@x.VIDEO"></video>
            </div>
        </div>
    </div>
    <div class="form-group">
        <label for="exampleInputName1">Sahne Adı</label><br />
        <textarea style="width:100%; height:30px;" readonly>@x.TEXT</textarea>
    </div>
}
<a style="color:#ffffff;" class="btn btn-light" href="/LanetlerKralıAdmin/Error">Geri Dön</a>

