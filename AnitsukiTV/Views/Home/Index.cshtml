    var swiper = new Swiper(".mySwiper", {
        spaceBetween: 30,
        loop: true,
        effect: "fade",
        centeredSlides: true,
        autoplay: {
            delay: 10000,
            disableOnInteraction: false
        },
        pagination: {
            el: ".swiper-pagination",
            clickable: true
        },
        navigation: {
            nextEl: ".swiper-button-next",
            prevEl: ".swiper-button-prev"
        },
        on: {
            autoplayTimeLeft(s, time, progress) {
                progressCircle.style.setProperty("--progress", 1 - progress);
                progressContent.textContent = `${Math.ceil(time / 1000)}s`;
            }
        }
    });
</script>

<script>
    const videoElements = document.querySelectorAll(".splide__slide .video");
    const actionInProgress = {};

    function isMobileDevice() {
        return /Mobi|Android|iPhone|iPad|iPod/i.test(navigator.userAgent);
    }

    function loadVideo(video) {
        if (video.dataset.src) {
            video.src = video.dataset.src;
            video.load();
        }
    }

    function updateIconState(element, condition, trueIcon, falseIcon) {
        if (condition) {
            element.innerHTML = trueIcon;
        } else {
            element.innerHTML = falseIcon;
        }
    }

    function initializeVideo(video) {
        let isMuted = true;
        video.muted = isMuted;

        const videoContainer = video.parentElement;
        const muteButton = videoContainer.querySelector('.mute-button');
        const textSecretButton = videoContainer.querySelector('.text-secret-button');
        const watchlater = videoContainer.querySelector('.watchlater');
        const favorite = videoContainer.querySelector('.favorite');
        const animeInfo = videoContainer.querySelector('.anime-info');

        if (!isMobileDevice()) {
            // Mobil olmayan cihazlar için yapılandırma
            if (muteButton) muteButton.style.display = 'none';
            if (animeInfo) animeInfo.style.display = 'none';
            if (watchlater) watchlater.style.display = 'none';
            if (favorite) favorite.style.display = 'none';

            const observer = new IntersectionObserver(entries => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        loadVideo(video);
                        observer.unobserve(entry.target);
                    }
                });
            });
            observer.observe(video);

            videoContainer.addEventListener('mouseenter', () => {
                if (video.paused) video.play().catch(console.error);
                video.classList.add("active");
                if (muteButton) muteButton.style.display = 'block';
                if (animeInfo) animeInfo.style.display = 'block';
                if (textSecretButton) textSecretButton.style.display = 'block';
                if (watchlater) watchlater.style.display = 'block';
                if (favorite) favorite.style.display = 'block';
            });

            videoContainer.addEventListener('mouseleave', () => {
                if (!video.paused) video.pause();
                video.classList.remove("active");
                if (muteButton) muteButton.style.display = 'none';
                if (animeInfo) animeInfo.style.display = 'none';
                if (textSecretButton) textSecretButton.style.display = 'none';
                if (watchlater) watchlater.style.display = 'none';
                if (favorite) favorite.style.display = 'none';
            });

            // Mute butonu kontrolü
            if (muteButton) {
                muteButton.addEventListener('click', (event) => {
                    event.stopPropagation();
                    event.preventDefault();
                    isMuted = !isMuted;
                    video.muted = isMuted;
                    updateIconState(muteButton, isMuted, '<i class="bi bi-volume-mute"></i>', '<i class="bi bi-volume-up"></i>');
                    if (!isMuted) video.volume = 0.5;
                });
            }

            // Watchlater ve Favorite butonları için AJAX çağrısı yaparak durumu kontrol et
            if (watchlater) {
                checkStatus(videoContainer.dataset.animeid, 'WatchLater', watchlater, '<i style="color:#ffffff;" class="bi bi-clock-fill"></i>', '<i style="color:#ffffff;" class="bi bi-clock"></i>');
                watchlater.addEventListener('click', (event) => {
                    event.stopPropagation();
                    event.preventDefault();
                    const animeID = videoContainer.dataset.animeid;
                    if (!actionInProgress[animeID + 'WatchLater']) {
                        actionInProgress[animeID + 'WatchLater'] = true;
                        addWatchLater(animeID, watchlater).finally(() => {
                            actionInProgress[animeID + 'WatchLater'] = false;
                        });
                    }
                });
            }

            if (favorite) {
                checkStatus(videoContainer.dataset.animeid, 'Favorite', favorite, '<i style="color:#ffffff;" class="bi bi-heart-fill"></i>', '<i style="color:#ffffff;" class="bi bi-heart"></i>');
                favorite.addEventListener('click', (event) => {
                    event.stopPropagation();
                    event.preventDefault();
                    const animeID = videoContainer.dataset.animeid;
                    if (!actionInProgress[animeID + 'Favorite']) {
                        actionInProgress[animeID + 'Favorite'] = true;
                        addFavorite(animeID, favorite).finally(() => {
                            actionInProgress[animeID + 'Favorite'] = false;
                        });
                    }
                });
            }

            // Text secret butonu kontrolü
            if (textSecretButton) {
                let isInfoVisible = true;
                textSecretButton.addEventListener('click', (event) => {
                    event.stopPropagation();
                    event.preventDefault();
                    if (isInfoVisible) {
                        animeInfo.style.transform = 'scale(1.025)';
                        animeInfo.style.opacity = '0';
                        textSecretButton.innerHTML = '<i class="bi bi-card-text"></i>';
                    } else {
                        animeInfo.style.transform = 'scale(1.025)';
                        animeInfo.style.opacity = '1';
                        textSecretButton.innerHTML = '<i class="bi bi-card-text"></i>';
                    }
                    isInfoVisible = !isInfoVisible;
                });
            }

        } else {
            // Mobil cihazlar için elementleri gizleme
            video.style.display = 'none';
            if (muteButton) muteButton.style.display = 'none';
            if (textSecretButton) textSecretButton.style.display = 'none';
            if (watchlater) watchlater.style.display = 'none';
            if (favorite) favorite.style.display = 'none';
        }
    }

    function checkStatus(animeID, action, element, trueIcon, falseIcon) {
        $.ajax({
            type: "GET",
            url: `/AnimeDetail/Check${action}?animeID=${animeID}`,
            success: function(data) {
                updateIconState(element, data.isActive, trueIcon, falseIcon);
            }
        });
    }

    // Sayfa yüklendiğinde her video için durumu kontrol et
    $(document).ready(function() {
        videoElements.forEach(initializeVideo);
        // Ek olarak, AJAX ile her ikonun durumunu kontrol edin:
        $('.favorite, .watchlater').each(function() {
            var action = $(this).data('action');
            var animeID = $(this).closest('.video-container').data('animeid');
            checkStatus(animeID, action.charAt(0).toUpperCase() + action.slice(1), this,
                action === 'favorite' ? '<i style="color:#ffffff;" class="bi bi-heart-fill"></i>' : '<i style="color:#ffffff;" class="bi bi-clock-fill"></i>',
                action === 'favorite' ? '<i style="color:#ffffff;" class="bi bi-heart"></i>' : '<i style="color:#ffffff;" class="bi bi-clock"></i>'
            );
        });
    });

    function addFavorite(animeID, elem) {
    // Eğer bir işlem zaten yapılıyorsa, yeni bir işlem başlatma
    if (elem.dataset.actionInProgress === 'true') return Promise.resolve();

    elem.dataset.actionInProgress = 'true'; // İşlem başladı
    return new Promise((resolve, reject) => {
        $.ajax({
            type: "POST",
            url: "/AnimeDetail/AddFavorite",
            data: { animeID: animeID },
            success: function (data) {
                if (data.success) {
                    updateIconState(elem, data.isFavorite, '<i style="color:#ffffff;" class="bi bi-heart-fill"></i>', '<i style="color:#ffffff;" class="bi bi-heart"></i>');
                    if (data.isFavorite) {
                        toastr["success"](data.message, "Ekleme işlemi");
                    } else {
                        toastr["error"](data.message, "Çıkarma işlemi");
                    }
                    resolve(data);
                } else {
                    window.location.href = "@Url.RouteUrl("Giriş")";
                    reject();
                }
            },
            error: reject,
            complete: function() {
                elem.dataset.actionInProgress = 'false'; // İşlem bitti
            }
        });
    });
}

// Daha sonra izle ekleme fonksiyonu
function addWatchLater(animeID, element) {
