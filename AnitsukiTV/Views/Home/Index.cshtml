        .user ul li a:hover {
            background-color: #ffffff;
            color: #000000 !important;
        }

        header #nabermudur ul {
            max-height: 300px;
            overflow-y: auto;
            width: 300px;
        }

            header #nabermudur ul .dracula {
                position: relative;
            }

                header #nabermudur ul .dracula::after {
                    content: "";
                    display: block;
                    position: absolute;
                    width: 90%;
